#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int count[100]={0};
    int n;
    cin>>n;
    int ar[n];
    for(int i=0;i<n;i++)
    {cin>>ar[i];count[ar[i]]++;}
    for(int i=0;i<100;i++)
        {
        cout<<count[i]<<" ";
    }
    
    return 0;
}
