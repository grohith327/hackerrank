#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int v,n;
    cin>>v>>n;
    int arr[n];
    for(int i=0;i<n;i++)
        {
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
        {
        if(arr[i]==v)
            {
            cout<<i;
            break;
        }
    }
    return 0;
}
